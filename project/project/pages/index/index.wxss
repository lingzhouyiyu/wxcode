/**index.wxss**/

.container {
  display: flex;
  flex-flow: column nowrap;
  margin: 0;
  padding: 0;
  font-size: 26rpx;
  overflow: hidden;
  text-overflow: ellipsis;
}

.partlineone {
  width: 100%;
  height: 15rpx;
  background-color: #f3f2f0;
}

.partlinetwo {
  width: 100%;
  height: 20rpx;
  background-color: #f3f2f0;
}

.partlinethree {
  width: 100%;
  height: 40rpx;
  background-color: #f3f2f0;
}

/* 头部样式 */

.header {
  width: 92%;
  height: auto;
  display: flex;
  flex-direction: row;
  padding: 10rpx 0 10rpx 0;
  align-items: center;
  margin: 0 20rpx 0 20rpx;
}

/* 定位图片 */

.Locationimg {
  width: 25rpx;
  height: 30rpx;
  margin: 0;
  padding: 0;
}

/* 通知图片 */

.noticeimg {
  width: 35rpx;
  height: 30rpx;
  position: absolute;
  right: 35rpx;
  margin-left: 30rpx;
}

.search {
  width: 71%;
  display: flex;
  flex-direction: row;
  justify-content: flex-start;
  margin-left: 21rpx;
  align-items: center;
  background-color: #f2f3f6;
  border-radius: 5px;
  padding-left: 10rpx;
  /* background-color: gray; */
}

/* 搜索框 */

.searchinput {
  border-style: none;
  color: #abaaab;
  height: 60rpx;
  width: 100%;
  margin-left: 5rpx;
  /* background-color: yellow; */
  display: flex;
  justify-content: flex-start;
  align-items: center;
  font-size: 24rpx;
}

/* 搜索图片 */

.searchimg {
  width: 30rpx;
  height: 30rpx;
  margin-left: 5rpx;
  /* background-color: red; */
}

/* 导航轮播图 */

.bannerimg {
  height: 320rpx;
  width: 92%;
  /* margin-top: 15rpx; */
  margin: 30rpx 20rpx 0 20rpx;
}

.bannerimg img {
  width: 100%;
  height: 100%;
}

.swiper {
  width: 100%;
  height: 100%;
  /* background-color: red; */
  overflow: visible;
  
}

