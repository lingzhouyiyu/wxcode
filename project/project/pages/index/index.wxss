.hotsign {
  width: 0;
  height: 0;
  border-color: #f2f2f2 transparent;
  border-width: 0 0 40rpx 40rpx;
  border-style: solid;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 200;
}

.hotsigns {
  width: 0;
  height: 0;
  border-color: #ff4b50 transparent;
  border-width: 0 0 110rpx 110rpx;
  border-style: solid;
  position: absolute;
  right: 0;
  bottom: 0;
  z-index: 100;
}

.signTxt {
  width: 80rpx;
  height: 30rpx;
  color: #fff;
  font-weight: bold;
  position: absolute;
  text-align: center;
  line-height: 30rpx;
  right: -4rpx;
  bottom: 26rpx;
  z-index: 300;
  transform: rotate(-45deg);
}

