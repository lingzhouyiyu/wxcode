  font-size: 24rpx;
  color: rgb(102, 102, 102);
}

.itemtop_right  .Score {
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  display: flex;
  align-items: center;
}

.itemtop_right  .Score  text {
  margin-left: 20rpx;
  font-size: 23rpx;
}

.starimg {
  width: 26rpx;
  height: 26rpx;
  margin-right: 10rpx;
}

.itemtop_right .renzhengimg {
  height: 31rpx;
  width: 81rpx;
}

