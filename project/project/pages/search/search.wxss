.container {
  position: relative;
}

.search {
  width: 92%;
  display: flex;
  flex-direction: row;
  align-items: center;
  background-color: #f2f3f6;
  border-radius: 5px;
  padding-left: 10rpx;
}

.partline {
  margin-top: 20rpx;
}

.historyview {
  width: 100%;
  height: auto;
  position: absolute;
  top: 85rpx;
  z-index: 9999;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  align-items: center;
  background-color: #fff;
}

.historyitem {
  width: 92%;
  height: 80rpx;
  border-bottom: 3rpx solid #f3f2f0;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

.clearallimage {
  width: 30rpx;
  height: 30rpx;
  margin-right: 20rpx;
}
.clearimg{
   width: 60rpx;
  height: 60rpx;
}
.historyitem text {
  font-size: 28rpx;
}

.historybottom {
  width: 100%;
  height: 70rpx;
  background-color: #f3f2f0;
  margin-top: -3rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  font-size: 28rpx;
}

.bottomitem {
  width: 92%;
  height: 100%;
  display: flex;
  justify-content: space-between;
  align-items: center;
}

/* 搜索框 */

.searchinput {
  border-style: none;
  color: #abaaab;
  height: 60rpx;
  width: 100%;
  margin-left: 5rpx;
}

/* 搜索图片 */

.searchimg {
  width: 30rpx;
  height: 30rpx;
  margin-left: 5rpx;
}

.footercontent {
  width: 100%;
  height: auto;
  display: flex;
  flex-direction: column;
  justify-content: flex-start;
  justify-items: center;
  margin-top: 20rpx;
}

.tab_comtainer {
  width: 92%;
  white-space: nowrap;
  height: 100rpx;
  line-height: 100rpx;
  margin: 0 30rpx 0 30rpx;
}

.tab_item {
  display: inline-block;
  height: 100rpx;
  line-height: 100rpx;
  padding: 0 32rpx;
  color: rgb(136, 136, 136);
  font-size: 28rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  background-color: white;
}

.active {
  /* color: rgb(50, 50, 50); */
  color: #65b5fc;
}

