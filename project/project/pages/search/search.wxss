}

.itemtopwraps {
  border-bottom: 0;
}

.itemtop {
  width: 92%;
  display: flex;
  justify-content: space-between;
  align-items: flex-start;
  /* border-bottom: 3rpx solid #f2f2f2; *//* margin-top: 20rpx; */
  margin: 20rpx 30rpx 0 30rpx;
}

.itemtop_left {
  width: 102rpx;
  height: 102rpx;
  background-color: gray;
  padding: 0;
  margin: 0;
  border-radius: 50%;
  overflow: hidden;
}

.itemtop_right {
  width: 82%;
