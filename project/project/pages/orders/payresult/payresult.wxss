.container{
  font-size: 30rpx;
}
.resultimg {
  width: 100%;
  height: 400rpx;
  /* background-color: red; */
  display: flex;
  justify-content: center;
  align-items: center;
  padding-top: 50rpx;
}

.resultimg image {
  width: 500rpx;
  height: 250rpx;
}

.titleswrap {
  width: 80%;
  height: 90rpx;
  /* background-color: yellow; */
  display: flex;
  justify-content: center;
  align-items: center;
}

.leftline {
  width: 150rpx;
  border: 3rpx dashed #f3f2f0;
  margin-right: 15rpx;
}

.rightline {
  width: 150rpx;
  border: 3rpx dashed #f3f2f0;
  margin-left: 15rpx;
}

.titles {
  color: #62605b;
  font-size: 30rpx;
}

.detailes {
  width: 80%;
  height: auto;
  display: flex;
  align-items: baseline;
  justify-content: space-between;
  margin-top: 20rpx;
  font-size: 30rpx;
}

.spot {
  width: 10rpx;
  height: 10rpx;
  background-color: #00aefe;
  border-radius: 50%;
  margin-right: 10rpx;
}

.texts {
  width: 97%;
}

.button {
  width: 80%;
  height: 100rpx;
  display: flex;
  align-items: center;
  justify-content: space-between;
  /* background-color: red; */
  margin-top: 100rpx;
  font-size: 30rpx;
}

.Btn_1 {
  width: 283rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  /* background-color: yellow; */
  border-radius: 25px;
  border: 3rpx solid #62605b;
}

.Btn_2 {
  width: 283rpx;
  height: 80rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  background-color: #00AEFE;
  border-radius: 25px;
  color: #fff;
}
