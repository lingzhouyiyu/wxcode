.container {
  font-size: 30rpx;
  position: relative;
}

.partline {
  width: 100%;
  height: 8rpx;
  background-color: #f3f2f0;
}

.titleview {
  display: flex;
  align-items: center;
  justify-content: center;
  width: 85%;
  height: 120rpx;
  /* background-color: red; */
  margin-top: 100rpx;
}

.titleview_left {
  width: 8%;
  height: 100%;
  /* background-color: yellow; */
}

.titleview_left image {
  width: 44rpx;
  height: 44rpx;
  transform: rotate(180deg);
}

.titleview_right {
  width: 92%;
  height: 100%;
  /* background-color: gray; */
  font-size: 30rpx;
}

.Btn {
  width: 80%;
  height: 80rpx;
  background-color: #45abf6;
  border-radius: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  margin-top: 100rpx;
  margin-bottom: 60rpx;
  font-size: 28rpx;
}

.Btns {
  width: 80%;
  height: 80rpx;
  background-color: #928f8a;
  border-radius: 50rpx;
  display: flex;
  justify-content: center;
  align-items: center;
  color: #fff;
  font-size: 28rpx;
}

.yuanjiao {
  width: 100%;
  height: 200rpx;
  background-color: #fff;
  border-radius: 50%;
  z-index: 9999;
  position: fixed;
  bottom: 200rpx;
}

.logo {
  width: 100%;
  height: 300rpx;
  background-color: #45abf6;
  position: fixed;
  bottom: 0;
  margin: 0;
  padding: 0;
}

.logo text {
  width: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin-top: 180rpx;
  color: #fff;
  font-size: 38rpx;
}

.thecanvas {
  width: 100%;
  height: 180rpx;
  /* background-color: red; */
  position: fixed;
  bottom: 150rpx;
  z-index: 9999;
}
.imgview{
  width: 100%;
  height: 260rpx;
   /* background-color: red; */
   position: fixed;
   bottom: 0;
}
.imgview image{
width: 100%;
height: 100%;
}
