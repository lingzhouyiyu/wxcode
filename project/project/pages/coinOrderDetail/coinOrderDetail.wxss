
.itemwrap{
width: 100%;
height: 120rpx;
border-bottom: 3rpx solid #F3F2F0;
display: flex;
/* background-color: red; */
justify-content: center;
align-items: center;
}
.item{
width: 92%;
height: 100%;
/* background-color: yellow; */
display: flex;
justify-content: space-between;
align-items: center;
}
.itemleft{
  width: 80%;
  height: 100%;
  display: flex;
  flex-direction: column;
  /* background-color: gray; */
  align-items: flex-start;
  justify-content: center;
}
.title{
  font-size: 28rpx;
  font-weight: bold;
}
.time{
  margin-top: 10rpx;
  font-size: 24rpx;
}
.itemright{
  width: 20%;
  height: 100%;
  display: flex;
  /* background-color: green; */
  justify-content: flex-end;
  align-items: center;
font-size: 38rpx;
}
.greencolor{
  color: green;
}