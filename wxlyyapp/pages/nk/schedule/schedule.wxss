.container {
  height: 100%;
  background: #f5f5f5;
}

/* 共享view样式*/

.userstyle {
  padding: 25rpx 40rpx;
  background: #fff;
  border-width: 0rpx 0rpx 4rpx 0rpx;
  border-style: solid;
  border-color: #f5f5f5;
  display: flex;
  align-items: center;
}

.userTextStyle {
  color: #4a4a4a;
  font-size: 26rpx;
  line-height: 1.42857;
  flex: 1;
}

.delview {
  display: inline-block;
  width: 100%;
  padding-top: 100%;
  margin-top: -18%;
  margin-left: -12px;
  background: url(data:image/png;base64,) center center no-repeat;
  -webkit-background-size: 18px auto;
  background-size: 18px auto;
}

.upview { display: inline-block; width: 16px; height: 17px; background: url(data:image/png;base64,) 0 0 no-repeat; -webkit-background-size: 100% auto; background-size: 100% auto; margin-top: 8px;}


.downview { display: inline-block; width: 16px; height: 17px; background: url(data:image/png;base64,) 0 0 no-repeat; -webkit-background-size: 100% auto; background-size: 100% auto; margin-top: 8px;}

